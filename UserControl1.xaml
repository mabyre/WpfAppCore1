<UserControl x:Class="WpfAppCore1.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfAppCore1"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button Content="Display Log" HorizontalAlignment="Left" Margin="40,30,0,0" VerticalAlignment="Top" Click="ButtonDisplay_Click"/>
        <Button Content="Trace Log" HorizontalAlignment="Left" Margin="40,68,0,0" VerticalAlignment="Top" Click="ButtonTrace_Click"/>
        <TextBox x:Name="textBoxLog" HorizontalAlignment="Left" Margin="40,0,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Center" Width="559" Height="246"/>

    </Grid>
</UserControl>
